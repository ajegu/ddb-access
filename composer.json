{
    "name": "ajegu/ddb-access",
    "description": "A PHP AWS DynamoDB library to provide a way to access to DynamoDB data",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajegu\\DdbAccess\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Allan JEGU"
        }
    ],
    "require": {
        "aws/aws-sdk-php": "^3.212",
        "psr/log": "^3.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5"
    }
}
